<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="800.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gruppo22.rubrica.Controller.AddContactController">
   <children>
      <VBox alignment="CENTER" prefHeight="800.0" prefWidth="800.0">
         <children>
            <ScrollPane maxHeight="1000.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0">
              <content>
                <AnchorPane minHeight="1000.0" minWidth="800.0" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <VBox alignment="CENTER" maxHeight="1000.0" maxWidth="800.0" minHeight="800.0" minWidth="800.0" spacing="50.0">
                           <children>
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></ImageView>
                              <TextField fx:id="inputName" maxWidth="400.0" minWidth="400.0" promptText="Nome..." />
                              <TextField fx:id="inputSurname" maxWidth="400.0" promptText="Cognome..." />
                              <TextField fx:id="inputPhoneNumber_1" maxWidth="400.0" promptText="Numero di telefono N.1..." />
                              <TextField fx:id="inputEmail_1" maxWidth="400.0" promptText="E-mail N.1..." />
                              <TextField fx:id="inputDescription" maxWidth="400.0" promptText="Descrizione..." />
                              <TextField fx:id="inputPhoneNumber_2" layoutX="210.0" layoutY="522.0" maxWidth="400.0" promptText="Numero di telefono N.2..." />
                              <TextField fx:id="inputEmail_2" layoutX="210.0" layoutY="566.0" maxWidth="400.0" promptText="E-mail N.2..." />
                              <TextField fx:id="inputPhoneNumber_3" layoutX="210.0" layoutY="645.0" maxWidth="400.0" promptText="Numero di telefono N.3..." />
                              <TextField fx:id="inputEmail_3" layoutX="210.0" layoutY="732.0" maxWidth="400.0" promptText="E-mail N.3..." />
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
              </content>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </ScrollPane>
            <HBox alignment="CENTER" minHeight="200.0" minWidth="600.0">
               <children>
                  <Button fx:id="cancelButton" alignment="CENTER" minHeight="50.0" minWidth="100.0" mnemonicParsing="false" onAction="#handlerCancelButtonAction" text="Annulla">
                     <HBox.margin>
                        <Insets right="150.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="saveButton" alignment="CENTER" layoutX="10.0" layoutY="10.0" minHeight="50.0" minWidth="100.0" mnemonicParsing="false" onAction="#handlerSaveButton" text="Salva">
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
